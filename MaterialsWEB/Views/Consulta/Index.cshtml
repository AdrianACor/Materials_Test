@model IEnumerable<MaterialsWEB.Models.consultaP_Result>

@{
    ViewBag.Title = "Consulta";
}

<h2>Consulta</h2>

@using (Html.BeginForm(null, null, FormMethod.Get))
{
    <fieldset>
        <legend>Criterios de búsqueda</legend>
        @Html.Label("partnumber", "PartNumber: ")
        @Html.TextBox("partNumber")
        @Html.Label("cust", "Customer: ")
        @Html.TextBox("customer")
        <input type="submit" value="Buscar" />
    </fieldset>
}
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PartNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Customer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Building)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PartNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Customer)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Building)
            </td>
        </tr>
    }

</table>
<div>
@Html.ActionLink("Ir a Inicio", "Index", "Home")
</div>